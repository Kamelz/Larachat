{
    "name": "kamelz/larachat",
    "description": "Embed chat system into your laravel application.",
    "keywords": ["laravel","chat","php"],
    "authors": [
        {
            "name": "Mohamed Kamel",
            "email": "kamel1145@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "laravel/framework": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "autoload": {
       "psr-4": {
           "Kamelz\\Larachat\\": "src/Larachat"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamelz\\Larachat\\Tests\\": "tests/"

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kamelz\\Larachat\\LarachatServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
