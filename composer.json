{
    "name": "vendor/larachat",
    "description": "Embed chat system into your laravel application.",
    "keywords": ["laravel","chat","php"],
    "authors": [
        {
            "name": "Mohamed Kamel",
            "email": "kamel1145@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*",
        "srmklive/paypal": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
       "psr-4": {
            "Vendor\\larachat\\": "src/larachat"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\larachat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\larachat\\LarachatServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
